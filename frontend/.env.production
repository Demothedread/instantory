# Production Environment Configuration for Bartleby Frontend
# This file is specifically for production builds and deployment

# Source Map Configuration - CRITICAL: Disable to prevent source map errors
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=true

# Build Optimization
INLINE_RUNTIME_CHUNK=false
REACT_APP_BUILD_PATH=build

# Production API Configuration
REACT_APP_BACKEND_URL=https://bartleby-backend-mn96.onrender.com
REACT_APP_FRONTEND_URL=https://hocomnia.com
REACT_APP_PRODUCTION_DOMAIN=hocomnia.com

# Google OAuth Configuration (Production)
REACT_APP_GOOGLE_CLIENT_ID=700638306537-27jsc5c64hrjq6153mc5fll6prmgef4o.apps.googleusercontent.com
REACT_APP_GOOGLE_REDIRECT_URI=https://bartleby-backend-mn96.onrender.com/api/auth/google/callback

# Environment Settings
NODE_ENV=production
REACT_APP_ENV=production

# Build Performance
BROWSER=none
CI=true

# Webpack/ESLint Configuration
EXTEND_ESLINT=false
TSC_COMPILE_ON_ERROR=true
ESLINT_NO_DEV_ERRORS=true
SKIP_PREFLIGHT_CHECK=true

# Disable Development Features
REACT_APP_DEBUG=false
REACT_APP_VERBOSE_LOGGING=false

# Additional CORS Origins (comma-separated)
REACT_APP_ADDITIONAL_ORIGINS=https://www.hocomnia.com,https://bartleby.vercel.app

# Performance Optimizations
REACT_APP_PRELOAD_SCRIPTS=false
REACT_APP_LAZY_LOAD=true
